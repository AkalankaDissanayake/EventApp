@model IEnumerable<App.Entity.ReferenceData>

@{
    ViewBag.Title = "My Event List";
}
<div class="container">
    <h2>My Event List</h2>

    <p>
        @Html.ActionLink("Create New Evet", "AddEdit")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
                <td>
                    @Html.ActionLink("Edit", "AddEdit", new { EventID = item.ID }) 
                </td>
            </tr>
        }

    </table>
</div>